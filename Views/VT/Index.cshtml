
<h2>Q1</h2>

@for (int i = 0; i < ViewBag.data.Length; i++)
{
    @(ViewBag.data[i])
}

<h2>Q2</h2>

@for (int i = 0; i < ViewBag.data.Length; i++)
{
    @((ViewBag.data[i]) + " ")
}

<h2>Q3</h2>
<ul>
@for (int i = 0; i < ViewBag.data.Length; i++)
{
  <li>@(ViewBag.data[i])</li>
}
</ul>


<h2>Q1 Final</h2>

@foreach (var item in ViewBag.data)
{
    @item
}


<h2>Q2 Final</h2>

@foreach (var item in ViewBag.data)
{
    @(item+" ")
}

<h2>Q3 Final</h2>

<ul>

@foreach (var item in ViewBag.data)
{
  <li>@item</li>
}
</ul>